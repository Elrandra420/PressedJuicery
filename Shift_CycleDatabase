CREATE TABLE ProductionCycleData (
    id SERIAL PRIMARY KEY, -- Unique identifier for each record
    ShiftIndex INT NOT NULL, -- Foreign key to shift_log table
    DayIndex INT NOT NULL, -- Day index for the shift
    CycleIndex INT NOT NULL, -- Index of the cycle entry
    CycleTime NUMERIC, -- Cycle time in minutes
    CycleStatus VARCHAR(20), -- Status of the cycle ('passed', 'failed', etc.)
    SKU1 VARCHAR(20), -- SKU for the first product
    SKU2 VARCHAR(20), -- SKU for the second product
    SKU1Produced INT, -- Number of bottles produced in this cycle
    SKU2Produced INT, -- Number of bottles produced in this cycle
    BottlesDowned INT, -- Number of bottles downed in this cycle
    DateRecorded TIMESTAMP DEFAULT CURRENT_TIMESTAMP -- Timestamp when the record was created
);